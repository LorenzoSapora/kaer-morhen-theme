{
    "name": "terminalsio/kaer-morhen",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Terminalsio\\KaerMorhen\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Terminalsio\\KaerMorhen\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
